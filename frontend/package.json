{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "generate-proto": "grpc_tools_node_protoc --plugin=./node_modules/.bin/protoc-gen-ts --plugin=protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin --js_out=import_style=commonjs_strict,binary:src/proto --ts_out=src/proto --grpc_out=grpc_js:src/proto --proto_path=../proto ../proto/*.proto",
    "prod:bun": "BUILD_TARGET=bun vite build && PORT=5173 bun dist/index.js"
  },
  "devDependencies": {
    "@grpc/proto-loader": "^0.7.10",
    "@playwright/test": "^1.28.1",
    "@simplewebauthn/types": "^9.0.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/crypto-js": "^4.2.2",
    "@types/dropzone": "^5.7.8",
    "@types/eslint": "8.56.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/ms": "^0.7.34",
    "@types/msgpack": "^0.0.34",
    "@types/rethinkdb": "^2.3.21",
    "@types/secure-random": "^1.1.3",
    "@types/sockjs-client": "^1.5.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.4.20",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "grpc-tools": "^1.12.4",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "sass": "^1.72.0",
    "svelte": "^4.2.12",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "ts-protoc-gen": "^0.15.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.2.0"
  },
  "type": "module",
  "dependencies": {
    "@dicebear/collection": "^8.0.0",
    "@dicebear/core": "^8.0.0",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@grpc/grpc-js": "^1.10.3",
    "@hokaccha/sql-formatter": "^1.4.0",
    "@iconify/svelte": "^3.1.6",
    "@popperjs/core": "^2.11.8",
    "@simplewebauthn/browser": "^9.0.1",
    "@simplewebauthn/server": "^9.0.3",
    "@types/qs": "^6.9.12",
    "argon2": "^0.40.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "dropzone": "^6.0.0-beta.2",
    "google-protobuf": "^3.21.2",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.2.0",
    "joi": "^17.12.2",
    "json-with-bigint": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "meilisearch": "^0.38.0",
    "minio": "^7.1.3",
    "ms": "^2.1.3",
    "nodemailer": "^6.9.12",
    "postgres": "^3.4.3",
    "qs": "^6.12.0",
    "redis": "^4.6.13",
    "rethinkdb": "^2.4.2",
    "secure-random": "^1.1.2",
    "simplemde": "^1.11.2",
    "sockjs-client": "^1.6.1",
    "svelte-popperjs": "^1.3.2",
    "svelte-sonner": "^0.3.19"
  }
}
