{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "sync": "svelte-kit sync",
    "dev": "vite dev",
    "build": "pnpm run paraglide && vite build",
    "paraglide": "paraglide-js compile --project ./frontend.inlang --outdir ./src/locales/compiled",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint . --fix",
    "fmt": "prettier --write .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "postinstall": "paraglide-js compile --project ./frontend.inlang --outdir ./src/locales/compiled && svelte-kit sync"
  },
  "devDependencies": {
    "@inlang/paraglide-js": "1.8.0",
    "@inlang/paraglide-sveltekit": "^0.6.17",
    "@macfja/svelte-multi-adapter": "^1.0.2",
    "@playwright/test": "^1.44.0",
    "@skeletonlabs/skeleton": "^2.10.0",
    "@skeletonlabs/tw-plugin": "^0.4.0",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/adapter-cloudflare": "^4.4.0",
    "@sveltejs/adapter-netlify": "^4.2.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/adapter-vercel": "^5.3.0",
    "@sveltejs/kit": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@types/eslint": "^8.56.10",
    "@types/globalize": "^1.5.5",
    "@types/hammerjs": "^2.0.45",
    "@types/node": "^20.12.10",
    "@types/swagger-ui": "^3.52.4",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.38.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "sass": "^1.77.0",
    "svelte": "^4.2.16",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.7.1",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-tailwind-purgecss": "^0.3.3",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@neoconfetti/svelte": "^2.2.1",
    "@sveltejs/svelte-scroller": "^2.0.7",
    "@threlte/core": "^7.3.0",
    "globalize": "^1.7.0",
    "hammerjs": "^2.0.8",
    "lucide-svelte": "^0.378.0",
    "qr-code-styling": "github:kozakdenys/qr-code-styling",
    "svelte-cleavejs": "^1.0.3",
    "svelte-french-toast": "^1.2.0",
    "svelte-infinite-loading": "^1.3.8",
    "swagger-ui": "^5.17.6",
    "three": "^0.164.1"
  }
}
